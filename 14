public class Solution 
{
    public int solution(int n) 
    {        
        int i = 0;
        int j = 0;

        int answer = 0;

        while (i < n)
        {
            i++;
            j = n % i;

            if (j == 0)
            {
            answer += i;
            }            
        }
        return answer;
    }
}
