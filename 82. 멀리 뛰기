public class Solution
{
    public long solution(int n)
    {
        // 1, 2, 3, 5, 8, 13,,, 피보나치 수열

        // 1

        // 1 1 
        // 2

        // 1 1 1
        // 2 1
        // 1 2

        // 1 1 1 1 
        // 2 1 1 
        // 1 2 1 
        // 1 1 2
        // 2 2

        // 1 1 1 1 1
        // 2 1 1 1
        // 1 2 1 1 
        // 1 1 2 1 
        // 1 1 1 2 
        // 2 2 1
        // 2 1 2
        // 1 2 2 

        // 1 1 1 1 1 1
        // 2 1 1 1 1
        // 1 2 1 1 1 
        // 1 1 2 1 1 
        // 1 1 1 2 1 
        // 1 1 1 1 2
        // 2 2 1 1 
        // 2 1 2 1 
        // 2 1 1 2
        // 1 2 1 2 
        // 1 1 2 2            
        // 1 2 2 1
        // 2 2 2

        long[] cases = new long[n];

        if (n == 1)
        {
            return 1;
        }

        cases[0] = 1;
        cases[1] = 2;

        for (int i = 2; i < n; i++)
        {
            cases[i] = (cases[i - 2] + cases[i - 1]) % 1234567;
        }

        return cases[n - 1];
    }
}
