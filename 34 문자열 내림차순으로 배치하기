using System;

public class Solution 
{
    public string solution(string s) 
    {
        string answer = "";
        char[] arr = s.ToCharArray();
        
        Array.Sort(arr);
        Array.Reverse(arr);
        
         for (int i = 0; i < arr.Length; i++)
        {
            answer += arr[i];
        }
        
        return answer;
    }
}
