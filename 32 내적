using System;

public class Solution 
{
    public int solution(int[] a, int[] b) 
    {
        int answer = 0;
        int[] arr = new int[a.Length];

        for (int i = 0; i < a.Length; i++)
        {
            arr[i] = a[i] * b[i];
            answer += arr[i];                    
        }
        return answer;
    }
}
